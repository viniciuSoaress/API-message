{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.ts"
    },
    {
      "src": "/message/(.*)",
      "dest": "src/router/RouterMessage.ts"
    },
    {
      "src": "/user/(.*)",
      "dest": "src/router/RouterUser.ts"
    }
  ],
  "env": {
    "DATABASE_URL": "file:./dev.db"
  }
}